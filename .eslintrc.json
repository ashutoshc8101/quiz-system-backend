{
  "env" : {
    "node": true,
    "es6": true
  },
  
  "parserOptions": {
    "sourceType": "module"
  },
  
  "rules": {
    "comma-dangle": 0,
    "no-cond-assign": [2,"except-parens"],
    "no-console": 0,
    "no-constant-condition": 1,
    "no-control-regex": 0,
    "no-debugger": 0,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 0,
    "no-empty": 0,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": [1,"all"],
    "no-extra-semi": 2,
    "no-func-assign": 1,
    "no-inner-declarations": 0,
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 0,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 1,
    "use-isnan": 1,
    "valid-jsdoc": 0,
    "valid-typeof": 2,
    "no-unexpected-multiline": 1,
    "accessor-pairs": 0,
    "block-scoped-var": 0,
    "complexity": 0,
    "consistent-return": 0,
    "curly": 0,
    "default-case": 0,
    "dot-notation": 0,
    "dot-location": 0,
    "eqeqeq": [1,"smart"],
    "guard-for-in": 0,
    "no-alert": 0,
    "no-caller": 0,
    "no-div-regex": 0,
    "no-else-return": 1,
    "no-empty-label": 0,
    "no-eq-null": 1,
    "no-eval": 0,
    "no-extend-native": 0,
    "no-extra-bind": 0,
    "no-fallthrough": 0,
    "no-floating-decimal": 0,
    "no-implicit-coercion": 0,
    "no-implied-eval": 0,
    "no-invalid-this": 0,
    "no-iterator": 0,
    "no-labels": 0,
    "no-lone-blocks": 0,
    "no-loop-func": 0,
    "no-multi-spaces": 0,
    "no-multi-str": 0,
    "no-native-reassign": 0,
    "no-new-func": 0,
    "no-new-wrappers": 0,
    "no-new": 0,
    "no-octal-escape": 0,
    "no-octal": 0,
    "no-param-reassign": 0,
    "no-process-env": 0,
    "no-proto": 0,
    "no-redeclare": 0,
    "no-return-assign": 0,
    "no-script-url": 0,
    "no-self-compare": 0,
    "no-sequences": 0,
    "no-throw-literal": 0,
    "no-unused-expressions": 0,
    "no-useless-call": 1,
    "no-void": 1,
    "no-warning-comments": 0,
    "no-with": 0,
    "radix": 0,
    "vars-on-top": 0,
    "wrap-iife": 0,
    "yoda": 0,
    "strict": 0,
    "init-declarations": 0,
    "no-catch-shadow": 0,
    "no-delete-var": 1,
    "no-label-var": 0,
    "no-shadow-restricted-names": 0,
    "no-shadow": 0,
    "no-undef-init": 0,
    "no-undef": 2,
    "no-undefined": 0,
    "no-unused-vars": 2,
    "no-use-before-define": 0,
    "callback-return": 1,
    "handle-callback-err": 1,
    "no-mixed-requires": [2,false],
    "no-new-require": 1,
    "no-path-concat": 1,
    "no-process-exit": 2,
    "no-restricted-modules": 0,
    "no-sync": 0,
    "array-bracket-spacing": 0,
    "brace-style": 0,
    "camelcase": [1,{"properties":"always"}],
    "comma-spacing": 0,
    "comma-style": 0,
    "computed-property-spacing": 0,
    "consistent-this": 0,
    "eol-last": 0,
    "func-names": 0,
    "func-style": 0,
    "id-length": 0,
    "indent": 0,
    "key-spacing": 0,
    "lines-around-comment": 0,
    "linebreak-style": 0,
    "max-nested-callbacks": 0,
    "new-cap": 0,
    "new-parens": 0,
    "newline-after-var": 0,
    "no-array-constructor": 0,
    "no-continue": 0,
    "no-inline-comments": 0,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": [2,"smart-tabs"],
    "no-multiple-empty-lines": 0,
    "no-nested-ternary": 1,
    "no-new-object": 0,
    "no-spaced-func": 0,
    "no-ternary": 0,
    "no-trailing-spaces": 0,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 0,
    "object-curly-spacing": 0,
    "one-var": 0,
    "operator-assignment": 0,
    "operator-linebreak": 0,
    "padded-blocks": 0,
    "quote-props": 0,
    "quotes": 0,
    "id-match": 0,
    "semi-spacing": 0,
    "semi": [2,"always"],
    "sort-vars": 0,
    "space-after-keywords": 0,
    "space-before-blocks": 0,
    "space-before-function-paren": 0,
    "space-in-parens": 0,
    "space-infix-ops": 0,
    "space-return-throw-case": 0,
    "space-unary-ops": 0,
    "spaced-comment": 0,
    "wrap-regex": 0,
    "arrow-parens": 0,
    "arrow-spacing": 0,
    "constructor-super": 0,
    "generator-star-spacing": 0,
    "no-class-assign": 0,
    "no-const-assign": 0,
    "no-this-before-super": 0,
    "no-var": 1,
    "object-shorthand": [1,"always"],
    "prefer-const": 0,
    "prefer-spread": 1,
    "prefer-reflect": 0,
    "require-yield": 0,
    "max-depth": 0,
    "max-len": 0,
    "max-params": 0,
    "max-statements": 0,
    "no-bitwise": 0,
    "no-plusplus": 0
  }
}
